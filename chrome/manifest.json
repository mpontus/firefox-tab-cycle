{
  "manifest_version": 2,
  "name": "Tab Cycle",
  "version": "1.1",
  "description": "Keep active tab at the front",
  "homepage_url": "https://github.com/mpontus/tab-cycle",
  "background": {
    "scripts": [
      "handlers.js",
      "commands.js"
    ],
    "persistent": false
  },
  "commands": {
    "bury-active-tab": {
      "suggested_key": {
        "default": "Alt+N"
      },
      "description": "Cycle tabs forwards"
    },
    "raise-last-tab": {
      "suggested_key": {
        "default": "Alt+P"
      },
      "description": "Cycle tabs backwards"
    }
  }
}
